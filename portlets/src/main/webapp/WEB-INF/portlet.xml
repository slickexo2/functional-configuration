<?xml version="1.0" encoding="UTF-8"?>
<portlet-app version="2.0"
             xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">

	<portlet>
		<portlet-name>FunctionalConfigurationPortlet</portlet-name>
		<display-name>Functional Configuration Portlet</display-name>
		<portlet-class>org.exoplatform.portlets.functionalConfiguration.FunctionalConfigurationPortlet</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<supported-locale>fr</supported-locale>
		<portlet-info>
			<title>Functional Configuration Portlet</title>
			<keywords>Functional Configuration Portlets</keywords>
		</portlet-info>
	</portlet>

	<portlet>
		<portlet-name>HighlightSpacesPortlet</portlet-name>
		<display-name>Highlight spaces portlet</display-name>
		<portlet-class>org.exoplatform.portlets.highlightspaces.HighlightSpacesPortlet</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<supported-locale>fr</supported-locale>
		<portlet-info>
			<title>Highlighted spaces group</title>
			<keywords>Spaces group</keywords>
		</portlet-info>
	</portlet>
</portlet-app>
